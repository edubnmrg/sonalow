

<!-- <div class='marco50'> -->
<div style="width:100%;height:600px;margin:0px 0px 0px 0px;padding:0px 0px 0px 0px;border:0px 0px 0px 0px;background-color:#f7f1c3;">
  <div class="marco50" style="background-color:#f7f1c3;">

          <h1 style="margin: auto;">Pegar url: </h1>

          <form method=GET action='/props' style="margin: auto;">
          <input class='prop-input' type='text' name='query_url'>
          </form>

  </div>
<br>
  <div class="container ">
  <table class="table table-bordered table-striped table-hover">
    <thead style="background-color:#1b79bb;color:#ffffff">
        <th>Propiedad</th>
        <th>Visitas</th>

    </thead>
    <tbody>
      {{#each visitas}}
      <tr>
        <td class="col-md-12">
          <a href=/props?query_url={{this.texto}}>{{this.texto}}</a>
        </td>
        <td class="col-sm-1">
          {{this.numero}}
        </td>
      </tr>
      {{/each}}
    </tbody>

  </table>
  </div>
</div>


<style>
  .prop-input {
    width: 300px;
    height: 30px;
    line-height: 30px;
    font-size: 16px;
    padding: 10px;
  }
</style>
